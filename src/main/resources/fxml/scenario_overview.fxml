<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<AnchorPane maxHeight="500.0" maxWidth="800.0" prefHeight="500" prefWidth="800" style="-fx-background-color: #19182d;"
            stylesheets="@/css/styles.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.ui.controllers.scenario.ScenarioOverviewController">
    <ScrollPane fx:id="scrollPane" fitToWidth="true" layoutX="30" layoutY="100" prefHeight="500" prefWidth="800"
                style="-fx-background: transparent; -fx-background-color: transparent;">

        <VBox fx:id="testsList" prefWidth="800" spacing="30" style="-fx-background-color: transparent;">

            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>

        </VBox>

    </ScrollPane>


    <!-- Exit Button -->
    <JFXButton fx:id="backToOverViewButton" layoutX="700" layoutY="20" styleClass="jfx-button-custom" text="Back"/>

    <!-- Title -->
    <Text layoutX="50" layoutY="60" style="-fx-font-size: 28px; -fx-fill: #aaa9ff; -fx-font-weight: bold;"
          text="Scenario Results Overview"/>


</AnchorPane>

